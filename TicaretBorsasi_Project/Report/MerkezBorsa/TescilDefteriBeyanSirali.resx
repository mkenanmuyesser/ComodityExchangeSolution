  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>--declare @BaslangicTarihi date=null;
--declare @BitisTarihi date=null;
--declare @BaslangicSubeKodu char(2)='01';
--declare @BitisSubeKodu char(2)='99';

select 
B.BeyanKey,
B.BeyanTarihi,
(SUBSTRING(B.BeyanNo, PATINDEX('%[^0 ]%', B.BeyanNo + ' '), LEN(B.BeyanNo)))+' - '+(SUBSTRING(B.BeyanSatirNo, PATINDEX('%[^0 ]%', B.BeyanSatirNo + ' '), LEN(B.BeyanSatirNo))) as BeyanNo,
MK.Kod as MalinKodu,
MK.Adi as MalinCinsi,
B.BeyanMiktar as MalinMiktari,
B.Mensei as MalinMensei,
B.UretimYili as MalinUretimYili,
B.SaticiAd as SataninUnvanAd,
B.SaticiAdres as SataninAdresi,
Cast(TS.SicilNo as int) as AlaninSicilNo,
TS.Unvan as AlaninUnvanAd,
B.BeyanFaturaTarihi as FaturaNoTarihi,
B.BirimFiyat as BirimFiyat,
BT.Kod as Birim,
B.BeyanSatisTutari as Tutar,
B.SimsariyeMiktar as SatOrgHUcreti,
B.TescilMiktar as TescilUcreti,
B.BeyanTastik as TasdikUcreti,
(B.SimsariyeMiktar+B.TescilMiktar+B.BeyanTastik) as BorsaUcreti,
BS.Kod as SubeNo,
BET.BeyanTipAdi as BeyanNevi,
'T.C. '+BB.ProgramBaslik as BorsaAdi
--,* 
from MERKEZ_BORSA.BEYAN as B
left join MERKEZ_BORSA.TT_MADDE_KOD as MK
on B.MaddeKodKey=MK.MaddeKodKey
left join MERKEZ_BORSA.TUCCAR_SICIL as TS
on b.TuccarSicilKey=TS.TuccarSicilKey
left join MERKEZ_BORSA.TT_BIRIM_TIP as BT
on B.BirimTipKey=BT.BirimTipKey
left join MERKEZ_BORSA.TT_BORSA_SUBE as BS
on B.BorsaSubeKey=BS.BorsaSubeKey
left join MERKEZ_BORSA.TT_BEYAN_TIP as BET
on B.BeyanTipKey=BET.BeyanTipKey
cross join 
PROGRAM.BORSA_BILGI as BB
where (@BaslangicTarihi is null or BeyanTarihi&gt;=@BaslangicTarihi)
and (@BitisTarihi is null or BeyanTarihi&lt;=@BitisTarihi)
and (@BaslangicSubeKodu is null or CAST(BS.Kod as int)&gt;=@BaslangicSubeKodu)
and (@BitisSubeKodu is null or CAST(BS.Kod as int)&lt;=@BitisSubeKodu)
order by B.BeyanTarihi,B.BeyanNo,B.BeyanSatirNo asc


</value>

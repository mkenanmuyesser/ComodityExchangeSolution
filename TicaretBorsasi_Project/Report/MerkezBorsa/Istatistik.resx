  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>--declare @BaslangicTarihi date='2014.01.01';
--declare @BitisTarihi date='2014.02.28';
--declare @BaslangicSubeKodu int=null;
--declare @BitisSubeKodu int=null;
--declare @BaslangicMaddeKodu int=null;
--declare @BitisMaddeKodu int=null;
--declare @SatisSekli varchar(50)='HMS';

with AnaGrup as
(
select * from [MERKEZ_BORSA].[TT_MADDE_KOD] where Kod like '%0000'
), AltGrup as
(
select * from [MERKEZ_BORSA].[TT_MADDE_KOD] where Kod not like '%0000'
)
select MaddeKodKey,0 as SatirKod,AG.Kod,Adi as MaddeAdi,BS.Kod as BorsaSubeKod,BS.BorsaSubeAdi as BorsaSubeAdi,null as Muamele,null as Adet,null as EnAz,null as EnCok,null as Ortalama,null as Miktari,null as Birim,null as Bedeli,null as SatisSekli,0 as SatisSekliKey,'T.C. '+BB.ProgramBaslik as BorsaAdi from AnaGrup as AG
cross join 
PROGRAM.BORSA_BILGI as BB
cross join MERKEZ_BORSA.TT_BORSA_SUBE as BS
where 
(select COUNT(*) from MERKEZ_BORSA.BEYAN as B 
inner join MERKEZ_BORSA.TT_MADDE_KOD as MK
on MK.MaddeKodKey=B.MaddeKodKey
inner join [MERKEZ_BORSA].[TT_SATIS_SEKLI] as SS
on SS.SatisSekliKey=B.SatisSekliKey
where MK.Kod like ''+SUBSTRING(AG.Kod,1,2)+'%' 
and B.BorsaSubeKey=BS.BorsaSubeKey
and (@BaslangicTarihi is null or BeyanTarihi&gt;=@BaslangicTarihi)
and (@BitisTarihi is null or BeyanTarihi&lt;=@BitisTarihi)
and (@BaslangicSubeKodu is null or CAST(BS.Kod as int)&gt;=@BaslangicSubeKodu)
and (@BitisSubeKodu is null or CAST(BS.Kod as int)&lt;=@BitisSubeKodu)
and (@BaslangicMaddeKodu is null or CAST(AG.Kod as int)&gt;=@BaslangicMaddeKodu)
and (@BitisMaddeKodu is null or CAST(AG.Kod as int)&lt;=@BitisMaddeKodu)
and (@SatisSekli is null or @SatisSekli ='Tümü' or  Replace(SS.SatisSekliAdi,'.','_') = @SatisSekli)
)&gt;0
union all
select AG.MaddeKodKey,1,AG.Kod,AG.Adi,BS.Kod,BS.BorsaSubeAdi,Count(B.BeyanKey),null,MIN(B.BirimFiyat),MAX(B.BirimFiyat),AVG(B.BirimFiyat),SUM(B.BeyanMiktar),BT.Kod,SUM(B.BeyanSatisTutari),SS.SatisSekliAdi,SS.SatisSekliKey,'T.C. '+BB.ProgramBaslik from AltGrup as AG
inner join [MERKEZ_BORSA].[BEYAN] as B
on B.MaddeKodKey=AG.MaddeKodKey
inner join [MERKEZ_BORSA].[TT_SATIS_SEKLI] as SS
on SS.SatisSekliKey=B.SatisSekliKey
inner join [MERKEZ_BORSA].[TT_BIRIM_TIP] as BT
on BT.BirimTipKey=B.BirimTipKey
inner join [MERKEZ_BORSA].[TT_BORSA_SUBE] as BS
on BS.BorsaSubeKey=B.BorsaSubeKey
cross join 
PROGRAM.BORSA_BILGI as BB
where (@BaslangicTarihi is null or BeyanTarihi&gt;=@BaslangicTarihi)
and (@BitisTarihi is null or BeyanTarihi&lt;=@BitisTarihi)
and (@BaslangicSubeKodu is null or CAST(BS.Kod as int)&gt;=@BaslangicSubeKodu)
and (@BitisSubeKodu is null or CAST(BS.Kod as int)&lt;=@BitisSubeKodu)
and (@BaslangicMaddeKodu is null or CAST(AG.Kod as int)&gt;=@BaslangicMaddeKodu)
and (@BitisMaddeKodu is null or CAST(AG.Kod as int)&lt;=@BitisMaddeKodu)
and (@SatisSekli is null or @SatisSekli ='Tümü' or  Replace(SS.SatisSekliAdi,'.','_') = @SatisSekli)
group by AG.MaddeKodKey,AG.Kod,AG.Adi,BS.Kod,BS.BorsaSubeAdi,SS.SatisSekliAdi,SS.SatisSekliKey,BT.Kod,SS.SatisSekliAdi,BB.ProgramBaslik
order by BorsaSubeKod,Kod,SatisSekliKey asc,Muamele desc

</value>

  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select TS.SicilNo, TS.Unvan, TS.KayitTarihi, MG.MeslekAdi, KT.Adi as SirketNevi, (select STUFF((SELECT ','+ISNULL(AdSoyad, '') FROM [MERKEZ_BORSA].[FIRMA_YETKILI] FY WHERE FY.TuccarSicilKey=TS.TuccarSicilKey FOR XML PATH('')), 1, 1, '')) as Yetkililer, TS.Sermaye, D.Kod as Derece, D.Aidat, (select top 1 [FirmaAdres] from [MERKEZ_BORSA].[FIRMA_ADRES] as FA WHERE FA.TuccarSicilKey=TS.TuccarSicilKey and FA.FirmaAdresTipKey=2) as Adres, VD.VergiDairesiAdi, TS.VergiNo, TS.SicilKayitNo, FS.BabaAdi, FS.DogumYeri, YEAR(FS.DogumTarihi) as DogumYili, (select top 1 FirmaTelefonFax from [MERKEZ_BORSA].[FIRMA_TELEFON_FAX] as FTF WHERE FTF.TuccarSicilKey=TS.TuccarSicilKey and FTF.FirmaTelefonFaxTipKey=1) as Telefon, (select top 1 FirmaTelefonFax from [MERKEZ_BORSA].[FIRMA_TELEFON_FAX] as FTF WHERE FTF.TuccarSicilKey=TS.TuccarSicilKey and FTF.FirmaTelefonFaxTipKey=5) as Fax, case when TS.TerkinTarihi is not null then 'Yönetim Kurulunun '+convert(varchar, TS.TerkinTarihi, 103) +' gün ve '+ts.TerkinYKKNo+' sayılı kararıyla rekin edilmiştir.' else '' end as Durum, TS.Aciklama, 'T.C. '+PBB.BorsaAdi+' TİCARET BORSASI' as BorsaAdi, PBB.GenelSekreter from [MERKEZ_BORSA].[TUCCAR_SICIL] as TS inner join [MERKEZ_BORSA].[TT_MESLEK_GRUP] as MG on TS.MeslekGrupKey=MG.MeslekGrupKey inner join [MERKEZ_BORSA].[TT_KURULUS_TUR] as KT on TS.KurulusTurKey=KT.KurulusTurKey inner join [MERKEZ_BORSA].[TT_DERECE] as D on TS.DereceKey=D.DereceKey inner join [MERKEZ_BORSA].[TT_VERGI_DAIRE] as VD on TS.VergiDaireKey=VD.VergiDaireKey left join [MERKEZ_BORSA].[FIRMA_SAHIS] as FS on TS.TuccarSicilKey=FS.TuccarSicilKey cross join [PROGRAM].[BORSA_BILGI] as PBB</value>
